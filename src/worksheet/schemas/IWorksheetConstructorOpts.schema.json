    "JSZipFileOptions": {
      "type": "object",
      "properties": {
        "base64": {
          "description": "Set to `true` if the data is `base64` encoded. For example image data from a `<canvas>` element. Plain text and HTML do not need this option.",
          "type": "boolean"
        },
        "binary": {
          "description": "Set to `true` if the data should be treated as raw content, `false` if this is a text. If `base64` is used,\nthis defaults to `true`, if the data is not a `string`, this will be set to `true`.",
          "type": "boolean"
        },
        "date": {
          "description": "Enables basic storage and retrieval of dates and times.",
          "type": "string",
          "format": "date-time"
        },
        "compression": {
          "type": "string"
        },
        "comment": {
          "type": "string"
        },
        "optimizedBinaryString": {
          "description": "Set to `true` if (and only if) the input is a \"binary string\" and has already been prepared with a `0xFF` mask.",
          "type": "boolean"
        },
        "createFolders": {
          "description": "Set to `true` if folders in the file path should be automatically created, otherwise there will only be virtual folders that represent the path to the file.",
          "type": "boolean"
        },
        "dir": {
          "description": "Set to `true` if this is a directory and content should be ignored.",
          "type": "boolean"
        },
        "dosPermissions": {
          "description": "6 bits number. The DOS permissions of the file, if any.",
          "type": [
            "null",
            "number"
          ]
        },
        "unixPermissions": {
          "description": "16 bits number. The UNIX permissions of the file, if any.\nAlso accepts a `string` representing the octal value: `\"644\"`, `\"755\"`, etc.",
          "type": [
            "null",
            "string",
            "number"
          ]
        }
      }
