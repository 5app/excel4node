{
  "name": "excel4node",
  "version": "1.0.0",
  "description": "Library to create Formatted Excel Files.",
  "engines" : {
    "node": ">4.0.0"
  },
  "keywords": [
    "excel",
    "spreadsheet",
    "xlsx",
    "formatted",
    "styled",
    "report",
    "workbook",
    "ooxml"
  ],
  "main": "./distribution/index.js",
  "author": {
    "name": "Nater",
    "email": "nater@seas.harvard.edu"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.php"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/natergj/excel4node.git"
  },
  "bugs": {
    "url": "https://github.com/natergj/excel4node/labels/bug"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/tape/bin/tape -r babel-register ./tests/*.test.js",
    "build": "babel source --presets babel-preset-es2015 -s --out-dir distribution",
    "watch": "babel source -w --presets babel-preset-es2015 -s --out-dir distribution",
    "document": "jsdoc ./source -r -d docs"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "image-size": "0.4.0",
    "jszip": "2.5.0",
    "lodash": "4.3.0",
    "mime": "^1.3.4",
    "sloth-logger": "^1.0.2",
    "xmlbuilder": "4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-node4": "^2.0.3",
    "babel-register": "^6.5.1",
    "jsdoc-babel": "^0.1.0",
    "pretty-data": "^0.40.0",
    "source-map-support": "^0.4.0",
    "tape": "^4.4.0",
    "tape-promise": "^1.1.0",
    "xmldom": "^0.1.22",
    "xpath.js": "^1.0.6"
  }
}
