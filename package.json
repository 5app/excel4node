{
  "name": "excel4node",
  "version": "2.0.0-alpha.1",
  "description": "Library to create formatted MS Excel files",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rm -rf lib .coverage .nyc_output",
    "compile:verify": "tsc --noEmit",
    "commit:hook": "npm run compile:verify && npm run prettier:verify && npm test",
    "docs:watch": "docsify serve docs",
    "prepublish": "npm run commit:hook && npm run build",
    "prettier:verify": "prettier --check src/**/*.ts docs/**/*.md",
    "prettier:fix": "prettier --write src/**/*.ts docs/**/*.md",
    "lint": "tslint 'src/**/*.ts'",
    "sample": "ts-node --project tsconfig.json sample.ts",
    "test": "NODE_ENV=test mocha --require ts-node/register --watch-extensions ts \"src/**/*.test*\"",
    "test:watch": ""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/natergj/excel4node.git"
  },
  "keywords": [
    "excel",
    "formatted",
    "ooxml",
    "report",
    "spreadsheet",
    "styled",
    "typescript",
    "workbook",
    "xlsx"
  ],
  "author": {
    "name": "Nater Jorde",
    "email": "nater@iamnater.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natergj/excel4node/labels/bug"
  },
  "homepage": "https://github.com/natergj/excel4node#readme",
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/jszip": "3.1.6",
    "@types/mocha": "5.2.7",
    "chai": "4.2.0",
    "docsify-cli": "4.3.0",
    "husky": "3.0.0",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "jszip": "3.2.1",
    "xmlbuilder": "13.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run commit:hook",
      "pre-push": "npm run commit:hook"
    }
  }
}
