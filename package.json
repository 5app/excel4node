{
  "name": "excel4node",
  "version": "2.0.0-alpha.1",
  "description": "Library to create formatted MS Excel filies",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rm -rf lib .coverage .nyc_output",
    "compile:verify": "tsc --noEmit",
    "commit:hook": "npm run compile:verify && npm run prettier:verify && npm test",
    "prepublish": "npm run commit:hook && pkg-ok && npm run build",
    "prettier:verify": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "lint": "tslint 'src/**/*.ts'",
    "sample": "ts-node --project tsconfig.json sample.ts",
    "test": "NODE_ENV=test mocha --require ts-node/register --watch-extensions ts \"src/**/*.test*\"",
    "test:watch": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natergj/excel4node.git"
  },
  "keywords": [
    "excel",
    "formatted",
    "ooxml",
    "report",
    "spreadsheet",
    "styled",
    "typescript",
    "workbook",
    "xlsx"
  ],
  "author": {
    "name": "Nater Jorde",
    "email": "nater@iamnater.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natergj/excel4node/labels/bug"
  },
  "homepage": "https://github.com/natergj/excel4node#readme",
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/jszip": "3.1.5",
    "@types/mocha": "5.2.6",
    "chai": "4.2.0",
    "husky": "1.3.1",
    "mocha": "6.0.2",
    "nyc": "13.3.0",
    "pkg-ok": "2.3.1",
    "prettier": "1.16.4",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "jszip": "3.2.1",
    "xmlbuilder": "11.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run commit:hook",
      "pre-push": "npm run commit:hook"
    }
  }
}
