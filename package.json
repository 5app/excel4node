{
  "name": "excel4node",
  "version": "1.3.2",
  "description": "Library to create Formatted Excel Files.",
  "engines": {
    "node": ">4.0.0"
  },
  "keywords": [
    "excel",
    "spreadsheet",
    "xlsx",
    "formatted",
    "styled",
    "report",
    "workbook",
    "ooxml"
  ],
  "main": "./distribution/index.js",
  "author": {
    "name": "Nater",
    "email": "nater@seas.harvard.edu"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.php"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/natergj/excel4node.git"
  },
  "bugs": {
    "url": "https://github.com/natergj/excel4node/labels/bug"
  },
  "scripts": {
    "test":
      "nyc --reporter=lcov --require ts-node/register tape tests/**/*.test.ts | tap-spec; nyc report ---reporter=text",
    "cover": "",
    "build": "tsc",
    "watch": "tsc -w",
    "document": "jsdoc ./source -r -d docs",
    "prepublishOnly": "npm run build; npm run test"
  },
  "dependencies": {
    "image-size": "0.6.1",
    "jszip": "3.1.5",
    "lodash": "4.17.4",
    "mime": "2.0.3",
    "sloth-logger": "1.0.3",
    "xmlbuilder": "9.0.4"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "jsdoc": "^3.5.5",
    "nyc": "11.3.0",
    "source-map-support": "0.5.0",
    "tap-spec": "^4.1.1",
    "tape": "4.8.0",
    "tape-promise": "2.0.1",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2",
    "xmldom": "0.1.27",
    "xpath.js": "1.0.7"
  },
  "nyc": {
    "instrument": true,
    "sourceMap": true,
    "include": ["source/**/*.ts"],
    "extension": [".ts"],
    "reporter": ["text-summary", "html"]
  }
}
